<?xml version="1.0"?>
<!-- Example of launch rviz with a custom rviz config to have selected topics preloaded -->
<launch>
  <arg name="rviz_config" default="$(find-pkg-share wamv_gazebo)/config/rviz_vrx.rviz" />
  <node pkg="rviz2" exec="rviz2"
        name="wamv_visualization" args="-d $(var rviz_config)" />
</launch>
